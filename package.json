  "scripts": {
    "open": "open http://localhost:8080",
    "webpack": "cross-env NODE_ENV=development webpack-dev-server --config build-tools/config/webpack/webpack.conf.dev.js",
    "dev": "yarn run open && nodemon --watch ./src/asset-manifest.json --exec 'yarn run webpack'",
    "upload:assets": "cross-env NODE_ENV=production node build-tools/script/upload.cmd.js --assets",
    "build": "yarn run lint && cross-env NODE_ENV=production node build-tools/script/build.cmd.js",
    "analyze": "yarn run build --analyze",
    "deploy:iframe": "yarn run build --deploy",
    "deploy": "node build-tools/escape/checkTileId.cmd.js && yarn run build --deploy --escape",
    "freeform": "cross-env NODE_ENV=production node build-tools/script/freeform.cmd.js",
    "review": "node build-tools/script/review.js",
    "lint:js": "eslint src --ext .js --cache",
    "lint:flow": "flow status",
    "lint": "yarn run lint:js",
    "test": "NODE_ENV=test jest --config=build-tools/test/jest.config.js",
    "docs": "esdoc -c .esdoc.js && open ./docs/index.html",
    "csm": "node build-tools/escape/checkTileId.cmd.js && node src/track/csm.cmd.js",
    "generate:asin": "node build-tools/script/copy.asin.cmd.js --locale=us",
    "generate:graph": "node build-tools/escape/checkTileId.cmd.js && node src/track/graph.cmd.js",
    "upload:escape-script": "terser src/escape/script/createTag.js --output src/escape/script/createTag.bundle.js && node src/escape/upload.cmd.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint",
      "pre-push": "yarn run lint"
    }
  },
